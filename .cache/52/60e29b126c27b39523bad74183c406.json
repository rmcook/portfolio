in-width: $mobile) {\n    flex: 1;\n  }\n}\n\n.resume__column--30 {\n  flex: 3;\n}\n\n.resume__column--70 {\n  flex: 7;\n\n  @media screen and (min-width: $mobile) {\n    padding-left: 80px;\n  }\n}\n\n.resume__summary {\n  margin: 0 0 $text-spacing 0;\n\n  address {\n    padding-left: $text-indent;\n  }\n\n  a {\n    background-color: $white;\n  }\n}\n\n.resume__heading {\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n  margin-bottom: $text-spacing;\n\n  @media (min-width: $mobile) {\n    display: block;\n    margin-bottom: unset;\n  }\n}\n\n.resume__photo {\n  width: 200px;\n}\n\n.resume__skills, .resume__frameworks, .resume__tools {\n  padding-left: $text-indent;\n\n  li {\n    list-style-type: none;\n  }\n}\n\n.resume__download-link {\n  display: block;\n  height: 30px;\n  width: 100%;\n  max-width: 350px;\n  background-color: $yellow;\n  border-radius: 25px;\n  font-weight: 400;\n  line-height: 30px;\n  text-align: center;\n  text-decoration: none;\n  margin: $text-spacing auto;\n\n  @media (min-width: $mobile) {\n    max-width: 225px;\n    margin: $text-spacing 0;\n  }\n}\n","src/stylesheets/index.scss":"@import 'colors';\n@import 'hero';\n@import 'project';\n@import 'resume';\n\n* {\n  box-sizing: border-box;\n\n  :focus {\n    outline: 1px solid $light-blue !important;\n    outline-offset: 5px;\n  }\n}\n\nbody {\n  margin: 0;\n  font-size: 13px;\n  font-weight: 100;\n  color: $white;\n  font-family: 'Roboto', sans-serif;\n\n  @media(min-width: $mobile) {\n    font-size: 14px;\n  }\n}\n\n.content-wrapper {\n  max-width: $max-width;\n  margin: 0 auto;\n  padding: 40px 40px;\n\n  @media( min-width: $mobile) {\n    padding: 80px 80px;\n  }\n\n}","src/stylesheets/_variables.scss":"$mobile: 680px;\n$max-width: 1600px;\n$text-indent: 10px;\n$text-spacing: 40px;"},"lineCount":null}},"error":null,"hash":"f1e87083c4b89bd6a5bbb736dcad1e9c","cacheData":{"env":{}}}